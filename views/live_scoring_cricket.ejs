            data["FIRST_INNING"]["SCORECARD"]["CURRENT_OVER"] += obj.over_add + '-';
            data["FIRST_INNING"]["SCORECARD"]["WICKETS"] += 1;
            data["FIRST_INNING"]["OUTPLAYERS"].push(data["FIRST_INNING"]["STRIKER"]);
            data["FIRST_INNING"]["STRIKER"] = obj.batsman;
        }
