            data["FIRST_INNING"]["BALLER"]["OVERS"] += 0.1;
            data["FIRST_INNING"]["BALLER"]["OVERS"] = parseFloat(data["FIRST_INNING"]["BALLER"]["OVERS"].toFixed(1));
            //ADD ECONOMY FOR BALLER
            let runns = data["FIRST_INNING"]["BALLER"]["RUNS"];
