            let econ = (runns / over).toFixed(1) != NaN && (runns / over).toFixed(1) != Infinity ? parseFloat((runns / over).toFixed(1)) : 0;
            console.log(econ);
            data["FIRST_INNING"]["BALLER"]["ECON"] = econ;
            //UPDATE REMARKS FOR OVER
            //UPDATE SCORECARD👇🏻
